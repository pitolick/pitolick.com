{
	"name": "pitolick.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "run-p dev:svelte test:ui storybook",
		"dev:svelte": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:related",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:integration-ui": "playwright test --ui",
		"test:unit": "vitest",
		"test:related": "vitest related --run",
		"test:ui": "vitest --ui",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepare": "husky"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.3.3",
		"@playwright/test": "^1.28.1",
		"@storybook/addon-essentials": "^8.0.8",
		"@storybook/addon-interactions": "^8.0.8",
		"@storybook/addon-links": "^8.0.8",
		"@storybook/addon-svelte-csf": "^4.1.2",
		"@storybook/addon-themes": "^8.0.8",
		"@storybook/addon-viewport": "^8.0.9",
		"@storybook/blocks": "^8.0.8",
		"@storybook/svelte": "^8.0.8",
		"@storybook/sveltekit": "^8.0.8",
		"@storybook/test": "^8.0.8",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"@sveltejs/enhanced-img": "^0.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^5.0.1",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitest/ui": "^1.5.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^9.0.11",
		"jsdom": "^24.0.0",
		"lint-staged": "^15.2.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"storybook": "^8.0.8",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.4.3",
		"tailwindcss-animated": "^1.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.5.0"
	},
	"type": "module",
	"volta": {
		"node": "20.12.2"
	}
}
